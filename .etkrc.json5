{
    "input": "test/**/*.(js|handlebars)",
    "output": [
        ".translations.json",
        ".translations.po",
        ".translations.py"
    ],
    "parsers": {
        "handlebars": {
            "extensions": [
                "handlebars",
                "hbs"
            ]
        },
        "javascript": {
            "extensions": [
                "js",
                "ts",
                "jsx",
                "tsx"
            ],
            "func": [
                "gettext"
            ],
            "plural": "p"
        }
    }
}